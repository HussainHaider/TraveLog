</header>
<div class="content">
    <div class="container-fluid">
    {{#if Data}}
        <div class="row" style="border-bottom: 2px solid">
            <div class="col-md-1"></div>
            <div class="col-md-10" style="padding: 3%; box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);">
                <div class="card">
                    {{#if Data.Thumbnail}}
                        <video controls autoplay name="media" height="350"><source src="{{ Data.Link }}"></video>
                    {{ else }}
                        <img class="card-img-top" src="{{ Data.Link }}" alt="Card image cap" height="350">
                    {{/if}}
                    <div class="card-body">
                        <h1>User Visted the Place</h1>
                        <ul>
                            <li>Hussain Haider</li>
                            <li>Ali Haider</li>
                        </ul>
                    </div>
                    <div class="card-footer">
                        <div class="row">
                            <div class="col-md-8">
                                <span class="heading">User Rating</span>
                                {{#times 0 Data.Rating}}
                                    <span class="fa fa-star checked"></span>
                                {{/times }}
                                {{#times 5 Data.Rating}}
                                    <span class="fa fa-star"></span>
                                {{/times }}
                            </div>
                        </div>
                        <h3>User Comments</h3>
                        {{#each Data.Comments}}
                        <div class="row commentsSection">
                            <div class="col-md-3">
                                {{ userName }}
                            </div>
                            <div class="col-md-9">
                                {{ userComment }}
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    {{/if}}
    </div>

</div>
