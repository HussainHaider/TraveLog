<div class="content" style="margin-top: 5%;">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="list-group">
                    {{#if Data}}
                        {{#each Data}}
                        <a href="{{ ID }}" class="list-group-item list-group-item-action">{{ Name }}</a>
                        {{/each}}
                    {{else}}
                        <div class="col-md-12 mb-4" style="text-align: center">
                            <h1 style="color: firebrick">No Record Found</h1>
                        </div>
                    {{/if}}
                </div>
            </div>
        </div>
    </div>
</div>
